    end
end)

task.spawn(function()
    while task.wait() do
     if _G.Factory_Check then
      pcall(function()
        			if game:GetService("Workspace").Enemies:FindFirstChild("Core") then
                        _G.Farm_Factory = true

                        if _G.Farm_Lv then
						_G.Farm_Lv = false
                        end
                    end
                
                    if _G.Farm_Factory then
                        if game:GetService("Workspace").Enemies:FindFirstChild("Core") then
						for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
							if v.Name == "Core" and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                    TP(v.HumanoidRootPart.CFrame)
                                    EquipWeapon(MainWeaponName)
                                    BusoHaki()
                                    Click()
							end
						end
                    end
                end

					if game:GetService("ReplicatedStorage"):FindFirstChild("Core") then
                    if game:GetService("ReplicatedStorage")["Core"]:FindFirstChild("Humanoid") then

						_G.Farm_Factory = true

                        if _G.Farm_Lv then
						_G.Farm_Lv = false
                        end

                        TP(game:GetService("ReplicatedStorage")["Core"]:FindFirstChild("HumanoidRootPart").CFrame)
                    end
                end
            end)
        end
    end
end)

end

local Mastery = page1:addSection("Mastery")

Mastery:addToggle("Auto Farm Devil Fruit Mastery", nil, function(value)
_G.Farm_MasteryDf = value

if value == false then
    TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
    UnClip()
end
end)

task.spawn(function()
while task.wait() do
    if _G.Farm_MasteryDf then
    pcall(function()
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

    if _G.GetQuest then
    if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
        CheckLevel()
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuest, LevelQuest)
    end
end

    CheckLevel()
    if game:GetService("Workspace").Enemies:FindFirstChild(Ms) then
        for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
        if v.Name == Ms and v:FindFirstChild("Humanoid") then

        HealthMin = v.Humanoid.MaxHealth * SelectHealth / 100
        if v.Humanoid.Health > HealthMin then
        MasteryCheck = false

        TP(v.HumanoidRootPart.CFrame * Farm_Mode)
    
        EquipWeapon(MainWeaponName)
        BusoHaki()
        Click()

    elseif v.Humanoid.Health <= HealthMin then
    MasteryCheck = true

    TP(v.HumanoidRootPart.CFrame * Farm_Mode)
    EquipWeapon("Blox Fruit")
    BusoHaki()
end
end
end
end
end)
end
end
end)




Mastery:addToggle("Auto Farm Gun Mastery", nil, function(value)
    _G.Farm_MasteryGun = value

if value == false then
    TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
    UnClip()
end
end)

task.spawn(function()
while task.wait() do
if _G.Farm_MasteryGun then
pcall(function()
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

        CheckLevel()
        if _G.GetQuest then
        if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuest, LevelQuest)
        end
    end

        CheckLevel()
        if game:GetService("Workspace").Enemies:FindFirstChild(Ms) then
            for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
            if v.Name == Ms then

            HealthMin = v.Humanoid.MaxHealth * SelectHealth / 100

            if v.Humanoid.Health > HealthMin then
            MasteryCheck = true
            
            TP(v.HumanoidRootPart.CFrame * Farm_Mode)

            BusoHaki()
            EquipWeapon(MainWeaponName)
            Click()

        elseif v.Humanoid.Health <= HealthMin then
        MasteryCheck = false

        TP(v.HumanoidRootPart.CFrame * Farm_Mode)

        BusoHaki()
        EquipWeapon("Gun")
        Click()
        game:GetService("Players").LocalPlayer.Character[GunWeaponName].Cooldown.Value = 0

        local args = {
            [1] = v.HumanoidRootPart.CFrame,
            [2] = v.HumanoidRootPart.CFrame
        }

        game:GetService("Players").LocalPlayer.Character[GunName].RemoteFunctionShoot:InvokeServer(unpack(args))
            end
        end
        end
        end
    end)
end
end
end)


