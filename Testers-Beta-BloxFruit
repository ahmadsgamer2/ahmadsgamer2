spawn(function()
    while wait(.1) do
        if AutoFarmType == "[Default : Behind] Auto Farm Type : Above" then
            Farm_Mode = CFrame.new(0,DisFarm,-1) * CFrame.Angles(math.rad(-90),0,0)
        elseif AutoFarmType == "[Default : Behind] Auto Farm Type : Bellow" then
            Farm_Mode = CFrame.new(0,DisFarm,1) * CFrame.Angles(math.rad(90),0,0)
        elseif AutoFarmType == "[Default : Behind] Auto Farm Type : Behind" then
            Farm_Mode = CFrame.new(0,0,DisFarm) * CFrame.Angles(math.rad(0),0,0)
        end
    end
    end)
    
    FarmTable = {
    "[Default : Behind] Auto Farm Type : Above",
    "[Default : Behind] Auto Farm Type : Bellow",
    "[Default : Behind] Auto Farm Type : Behind"
    }
    
    AutoFarmType = FarmTable[3]

    FARMS:addDropdown("[Default : Behind] Auto Farm Type : Behind", FarmTable, function(value)
        AutoFarmType = value
    if value == "[Default : Bellow] Auto Farm Type : Bellow" then

            UI:Notify(HubName, "To Use This Type.\n You Must Sliding The Distance Lower Than 0")
        end
    end)

    MainWeaponList = {
        "Melee",
        "Sword",
        "Blox Fruit"
    } 

    MainWeaponName = 
    FARMS:addDropdown("Select Weapon : Melee", MainWeaponList, function(value)
        MainWeaponName = value
    end)

    _G.GetQuest = true
    FARMS:addToggle("Auto Farm Receive Quest", true, function(value)
    _G.GetQuest = value
    if value == false then
        TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
        UnClip()
    end
end)

    _G.BringMonster = true
    FARMS:addToggle("Bring Monster", true, function(value)
    _G.BringMonster = value

        if value == false then
            TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
            UnClip()
        end
    end)

    FarmSpeedTable = {
        "Auto Farm Speed : 45 %",
        "Auto Farm Speed : 100 %\n[Dead]"
    }

    FarmSpeed = "Auto Farm Speed : 100 %\n[Dead]"

    FARMS:addDropdown("Auto Farm Speed : 100 %\n[Dead]", FarmSpeedTable, function(value)
        FarmSpeed = value
    end)

