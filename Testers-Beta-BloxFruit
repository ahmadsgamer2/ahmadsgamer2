    FastAttackList = {
        "Slow",
        "Fast",
        "Extreme",
    }
        

    Main:Dropdown("Select Attack Type",FastAttackList, function(value)
        _G.FastAttack_Mode = value
        if _G.FastAttack_Mode == "Slow" then
            _G.Fast_Delay = 0.080
        elseif _G.FastAttack_Mode == "Fast" then
            _G.Fast_Delay = 0.029
        elseif _G.FastAttack_Mode == "Extreme" then
            _G.Fast_Delay = 0.05
        end
    end)
        
        if _G.FastAttack_Mode == nil then
        _G.FastAttack_Mode = "Fast"
        end
        
        Main:Toggle("Fast Attack",true,function(value)
            _G.FastAttack = value
            FastAttack = value
        end)
        
        spawn(function()
            while task.wait(_G.Fast_Delay) do
                if _G.FastAttack then 
