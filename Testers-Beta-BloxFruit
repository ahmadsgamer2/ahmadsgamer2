        end)

        game:GetService("Workspace").Enemies.ChildAdded:Connect(function() 
            for _,v2 in pairs(MonsTable) do
             table.remove(MonsTable, _)
         end
            
            for _,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                insert = true 
                for _,v2 in pairs(MonsTable) do
                 if v2 == v.Name then
                 insert = false end
                 end
                if insert then
             table.insert(MonsTable, v.Name) end 
            end
            MonsSelect:Refresh(MonsTable)
        end)

