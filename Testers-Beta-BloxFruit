    end)

    local BountyS = plypage:addSection("Bounty Setting")

    spawn(function()
    while wait(.1) do
        if BountyType == "[Default : Behind] Auto Farm Type : Above" then
            Bounty_Mode = CFrame.new(0,DisBount,-1) * CFrame.Angles(math.rad(-90),0,0)
        elseif BountyType == "[Default : Behind] Auto Farm Type : Bellow" then
            Bounty_Mode = CFrame.new(0,DisBount,1) * CFrame.Angles(math.rad(90),0,0)
        elseif BountyType == "[Default : Behind] Auto Farm Type : Behind" then
            Bounty_Mode = CFrame.new(0,0,DisBount) * CFrame.Angles(math.rad(0),0,0)
        end
    end
    end)
    
    BountyTable = {
    "[Default : Behind] Auto Farm Type : Above",
    "[Default : Behind] Auto Farm Type : Bellow",
    "[Default : Behind] Auto Farm Type : Behind"
    }
    
    BountyType = BountyTable[3]

    BountyS:addDropdown("[Default : Behind] Auto Farm Type : Behind", FarmTable, function(value)
        BountyType = value
    if value == "[Default : Bellow] Auto Farm Type : Bellow" then

            UI:Notify(HubName, "To Use This Type.\n You Must Sliding The Distance Lower Than 0")
        end
    end)

    BountyWeaponList = {}

    for i, v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
    if v:IsA("Tool") then
    table.insert(BountyWeaponList, v.Name)
    end
end

    BountyS:addDropdown("Select Weapon : None", BountyWeaponList, function(value)
        BountyWeaponName = value
    table.clear(BountyWeaponList)
        for i, v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
            if v:IsA("Tool") then
            table.insert(BountyWeaponList, v.Name)
            end
        end
    end)

    DisBount = 10
    BountyS:addSlider("Distance", 10, 1, 50, function(value)
        DisBount = value
    end)

    local plysection2 = plypage:addSection("Bounty")
    
    plysection2:addToggle("Auto Farm Bounty", nil,function(value)
        _G.AutoFarmBounty = value
        if value == false then

            _G.Z_Skill = false
            _G.X_Skill = false
            _G.C_Skill = false
            _G.V_Skill = false

            TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
            UnClip()
        end
    end)

    
    NextplySelect = false
    StartCheckTarget = false

    task.spawn(function()
        while task.wait() do
            if _G.AutoFarmBounty then
            pcall(function()
                    for i,v in pairs(game:GetService("Workspace").Characters:GetChildren()) do
                        if v.Name ~= game.Players.LocalPlayer.Name then
                            if v:WaitForChild("Humanoid").Health > 0 and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 17000 then
                                plyselecthunthelpold = v.Humanoid.Health
                                EquipWeapon(BountyWeaponName)
                                    NameTarget = v.Name

                                    if tostring(game.Players.LocalPlayer.Team) == "Pirates" then
                                        TP(v.HumanoidRootPart.CFrame * Bounty_Mode)
                                        NameTarget = v.Name

                                    elseif tostring(game.Players.LocalPlayer.Team) == "Marines" then
                                        if game.Players[NameTarget].Team ~= game.Players.LocalPlayer.Team then
                                            TP(v.HumanoidRootPart.CFrame * Bounty_Mode)
                                            NameTarget = v.Name
                                        end
                                    end

                                    spawn(function()
                                        if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 150 then
                                            StartCheckTarget = true
                                        end
                                    end)

                                    spawn(function()
                                        if (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude >= 60 then
                                            _G.Z_Skill = false
                                            _G.X_Skill = false
                                            _G.C_Skill = false
                                            _G.V_Skill = false
                                        elseif (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 60 then
                                            BusoHaki()
                                            Click()

                                            _G.Z_Skill = true
                                            _G.X_Skill = true
                                            _G.C_Skill = true
                                            _G.V_Skill = true
                                        end
                                    end)

                                    v.HumanoidRootPart.CanCollide = false

                                TargetSelectHunt = v.Humanoid
                                NameTarget = v.Name
                            end
                        end
                    end
                end)
            end
        end
    end)
    
    
    spawn(function()
        pcall(function()
            while wait() do
                if _G.AutoFarmBounty then
                    if TargetSelectHunt ~= nil then
                        if StartCheckTarget then
                            wait(6.5)
                            if TargetSelectHunt.Health == TargetSelectHunt.MaxHealth or TargetSelectHunt.Health >= plyselecthunthelpold then
                                NextplySelect = true
                                TargetSelectHunt = nil
                            end
                        end
                    end
                end
            end
        end)
    end)

task.spawn(function()
    while wait() do
        if _G.AutoFarmBounty then
        pcall(function()
                if game:GetService("Players").LocalPlayer.PlayerGui.Main.PvpDisabled.Visible == true then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("EnablePvp")
                end
            end)
            end
        end
    end)
                            

    plysection2:addToggle("Auto Farm Bounty Hop", nil,function(value)
        _G.AutoHop = value
    end)
    
    spawn(function()
        while wait() do
                if _G.AutoHop then
                    pcall(function()
                        wait(120)
                        Hop()
                end)
            end
        end
    end)

    plysection2:addButton("Go To Next Player", function()
        NextTarget = true
    end)

    plysection2:addButton("Check Your Current Bounty", function()
        UI:Notify("Impact Hub", "Your Current Bounty Is : "..(game:GetService("Players").LocalPlayer.leaderstats["Bounty/Honor"].Value))
    end)
    
    spawn(function()
        game:GetService("RunService").RenderStepped:Connect(function()
         pcall(function()
             if _G.Haki then
                if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
            end
        end
        end)
        end)       
    end)  
    spawn(function()
        game:GetService("RunService").RenderStepped:Connect(function()
         pcall(function()
     if _G.KillPlayer then
     TP(game.Players[Player].Character.HumanoidRootPart.CFrame * CFrame.new(0,5,8))
     end
     end)
     end)
     end)

     local raidesp = UI:addPage("Raid\nEsp", 7251993295)
     local raidsection = raidesp:addSection("Raid")
     
     raidsection:addToggle("Kill Aura", nil, function(value)
         _G.KillAura = value
         if value == false then
         TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
         end
     end)
     
     spawn(function()
         pcall(function() 
             while wait() do
                 if _G.KillAura then
                     if game:GetService("Players")["LocalPlayer"].PlayerGui.Main.Timer.Visible == true then
                         for i,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
                             if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                                 pcall(function()
                                     repeat task.wait()                                    
                                         v.Humanoid.Health = 0
                                         v.HumanoidRootPart.CanCollide = false
                                         sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius",math.huge)
                                     until not _G.KillAura or not v.Parent or v.Humanoid.Health <= 0
                                 end)
                             end
                         end
                     end
                 end
             end
         end)
     end)
     
     raidsection:addToggle("Auto Go To Next Island", nil, function(value)
         _G.AutoNextIsland = value
         if value == false then
         TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
         UnClip()
         end
     end)
     
     spawn(function()
             while wait() do
                  pcall(function()
                 if _G.AutoNextIsland then
                    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                    for i, v in pairs(game:GetService("Workspace")["_WorldOrigin"].Locations:GetChildren()) do
                        if string.find(v.Name, "Island") and v:IsA("Part") then
                        if v.Name == "Island 1" or "Island 2" or "Island 3" or "Island 4" or "Island 5" and v:IsA("Part") then
                            if (v.Position - plr.Position).Magnitude > 350 then
                                plr.CFrame = v.CFrame * CFrame.new(0,75,0)
                            else
                                TP(v.CFrame * CFrame.new(0,75,0))
                            end

                        elseif not string.find(v.Name, "Island") and v:IsA("Part") then
                            UI:Notify("Impact Hub", "Please Enter The Raid")
                           end
                         end
                     end
                 end
             end)
     end
    end)
     
     raidsection:addToggle("Auto Awake Devil Fruit", nil,function(value)
         _G.AutoAwake = value
     end)
     
     spawn(function()
         pcall(function()
             while wait() do
                 if _G.AutoAwake then
                     game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Awakener","Check")
                     game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Awakener","Awaken")
                 end
             end
         end)
     end)
     
     raidsection:addDropdown("Select Chips",{"Flame","Ice","Quake","Light","Dark","String","Rumble","Magma","Human: Buddha","Sand","Bird: Phoenix [ADVANCE]", "Dough [ADVANCE]"},function(value)
         _G.SelectChip = value
     end)
     
