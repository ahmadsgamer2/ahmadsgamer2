                if _G.GetQuest then
                    if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuest, QuestLv)
                    end
                end
                        CheckLevel()
                        if (CFrameMon.Position - plr.Position).Magnitude > 350 then
                            if FarmSpeed == "Auto Farm Speed : 100 %\n[Dead]" then
                            
                                CheckLevel()
                                if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value ~= SpawnValue then
                                        if not game:GetService("Workspace").Enemies:FindFirstChild(SaveSelectMons) then
                                            CheckLevel()
