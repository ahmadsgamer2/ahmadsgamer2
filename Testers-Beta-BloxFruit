                                            until v.Humanoid.Health <= 0 or not RipIndra
                                            Bringmob = false
                                            _G.FastAttack = false
                                            else
                                        end
                                    end
                                else
                                    toTarget(CFrame.new(-5524.53271, 313.800537, -2918.07422, 0.964194059, 0, 0.265197694, 0, 1, 0, -0.265197694, 0, 0.964194059))
                                end
                            end
                        end)
                    end
     end)

