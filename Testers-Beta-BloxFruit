    Title = "Select Island",
    Values = TeleportTable,
    Multi = false,
    Default = 1,
})

if World1 then
SelectIslandSet:SetValue("Marine1")
elseif World2 then
SelectIslandSet:SetValue("Mansion")
elseif World3 then
SelectIslandSet:SetValue("Hydra Island")
end

