    Title = "Attack Type",
    Values = FastAttack,
    Multi = false,
    Default = 1,
})

SetAttack:SetValue("Fast") 

SetAttack:OnChanged(function(a)
    SelectSpeedFast = a
    if SelectSpeedFast == "Slow" then
        _G.Fast_Delay = 0.080
    elseif SelectSpeedFast == "Fast" then
        _G.Fast_Delay = 0.029
    elseif SelectSpeedFast == "Extreme" then
        _G.Fast_Delay = 0
    end
end)

if SelectSpeedFast == nil then
    SelectSpeedFast = "Fast"
end

