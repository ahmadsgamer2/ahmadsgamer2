local CastleQuest = page1:addSection("Hunter")

CastleQuest:addToggle("Auto Farm Elite Boss", nil, function(value)
    _G.Farm_Elite = value
    if value == false then
        TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
        UnClip()
    end
end)

CastleQuest:addToggle("Auto Farm Elite Boss Hop", nil, function(value)
    _G.Farm_Elite_Hop = value
    if value == false then
        TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
        UnClip()
    end
end)

task.spawn(function()
    while task.wait() do
    if _G.Farm_Elite_Hop then
    pcall(function()
        local Repli = game:GetService("ReplicatedStorage")
        local Enemies = game:GetService("Workspace").Enemies
        local Sentence = "I don't have anything for you right now. Come back later."

    if not Repli:FindFirstChild("Deandre [Lv. 1750]") and not Enemies:FindFirstChild("Deandre [Lv. 1750]") and Repli.Remotes.CommF_:InvokeServer("EliteHunter") == Sentence then
        wait()
        UI:Notify(HubName, "Not Found Elite Boss !")
        Hop()

    elseif not Repli:FindFirstChild("Urban [Lv. 1750]") and not Enemies:FindFirstChild("Urban [Lv. 1750]") and Repli.Remotes.CommF_:InvokeServer("EliteHunter") == Sentence then
        wait()
        UI:Notify(HubName, "Not Found Elite Boss !")
        Hop()

    elseif not Repli:FindFirstChild("Diablo [Lv. 1750]") and not Enemies:FindFirstChild("Diablo [Lv. 1750]") and Repli.Remotes.CommF_:InvokeServer("EliteHunter") == Sentence then
        wait()
        UI:Notify(HubName, "Not Found Elite Boss !")
        Hop()
    end
    end)
end
end
end)

CastleQuest:addToggle("Auto Farm Player Hunter", nil, function(value)
    _G.Farm_Players = value
    if value == false then
        _G.Z_Skill = false
        _G.X_Skill = false
        _G.C_Skill = false
        _G.V_Skill = false

        TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
        UnClip()
    end
end)

task.spawn(function()
    while task.wait() do
    if _G.Farm_Elite then
    pcall(function()

    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
    local QuestText = game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text
    local Repli = game:GetService("ReplicatedStorage")

    if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then 
    if not string.find(QuestText, "Deandre") and not string.find(QuestText, "Urban") and not string.find(QuestText, "Diablo") then

            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
        end
    end

    if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then 
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("EliteHunter")

    if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("EliteHunter") == "I don't have anything for you right now. Come back later." then
        wait(2)
        UI:Notify(HubName, "Not Found Elite Boss !")
        end
    end

    local QuestText = game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text
    local Repli = game:GetService("ReplicatedStorage")

    if string.find(QuestText, "Deandre") or string.find(QuestText, "Urban") or string.find(QuestText, "Diablo") then
    if Repli:FindFirstChild("Deandre [Lv. 1750]") then
        TP(Repli:FindFirstChild("Deandre [Lv. 1750]").HumanoidRootPart.CFrame * CFrame.new(25,25,25))

    elseif Repli:FindFirstChild("Urban [Lv. 1750]") then
        TP(Repli:FindFirstChild("Urban [Lv. 1750]").HumanoidRootPart.CFrame * CFrame.new(25,25,25))

    elseif Repli:FindFirstChild("Diablo [Lv. 1750]") then
        TP(Repli:FindFirstChild("Diablo [Lv. 1750]").HumanoidRootPart.CFrame * CFrame.new(25,25,25))
    end

    local Enemies = game:GetService("Workspace").Enemies
    if Enemies:FindFirstChild("Deandre [Lv. 1750]") or Enemies:FindFirstChild("Urban [Lv. 1750]") or Enemies:FindFirstChild("Diablo [Lv. 1750]") then
    
    for i, v in pairs(Enemies:GetChildren()) do
    if v.Name == "Deandre [Lv. 1750]" or v.Name == "Urban [Lv. 1750]" or v.Name == "Diablo [Lv. 1750]" and v:FindFirstChild("Humanoid") then
    if v.Humanoid.Health > 0 then
    TP(v.HumanoidRootPart.CFrame * Farm_Mode)
    BusoHaki()
    EquipWeapon(MainWeaponName)
    Click()
                        end
                    end
                end
            end
        end
    end)
end
end
end)

task.spawn(function()
    while task.wait() do
    if _G.Farm_Players then
    pcall(function()
    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
        
    if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then 
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("PlayerHunter")

    if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("PlayerHunter") == "I don't have anything for you right now. Come back later." then
        wait(2)
        UI:Notify(HubName, "Not Found Player ! or Cooldowning...")
        end
    end

    local QuestText = game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text

    if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then 
    for i, v in pairs(game:GetService("Players"):GetChildren()) do

    local QuestText = game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text

    if string.find(QuestText, v.Name) and v:IsA("Player") then
    
    if game:GetService("Players")[v.Name].Character.Humanoid.Health > 0 then
    TP(game:GetService("Players")[v.Name].Character.HumanoidRootPart.CFrame * Farm_Mode)
    BusoHaki()
    EquipWeapon(MainWeaponName)

    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

    if (game:GetService("Players")[v.Name].Character.HumanoidRootPart.Position - plr.Position).Magnitude > 60 then
        _G.Z_Skill = false
        _G.X_Skill = false
        _G.C_Skill = false
        _G.V_Skill = false
    end

    if (game:GetService("Players")[v.Name].Character.HumanoidRootPart.Position - plr.Position).Magnitude <= 60 then
    wait()
    
    game:GetService'VirtualUser':CaptureController()
    game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672), game:GetService("Workspace").Camera.CFrame)

    _G.Z_Skill = true
    _G.X_Skill = true
    _G.C_Skill = true
    _G.V_Skill = true

    wait(8)
    if game:GetService("Players")[v.Name].Character.Humanoid.Health == game:GetService("Players")[v.Name].Character.Humanoid.MaxHealth then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
                        end
                    end
                end
            end
        end
    end
    end)
end
end
end)

local BossFarm = page1:addSection("Boss")

local BossTable = {}   
for i, v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
	if string.find(v.Name, "Boss") then
		if v.Name == "Ice Admiral [Lv. 700] [Boss]" then
		
		else
			table.insert(BossTable, v.Name)
		end
	end
end

BossFarm:addDropdown("Select Boss : None", BossTable, function(value)
    SelectBoss = value
end)

BossFarm:addToggle("Auto Farm Boss", nil, function(value)
    _G.Farm_Boss = value
if value == false then
        TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
        UnClip()
    end
end)

task.spawn(function()
    while task.wait() do
     if _G.Farm_Boss then
        pcall(function()

			    CheckBoss()
				if MsBoss == "Soul Reaper [Lv. 2100] [Raid Boss]" or MsBoss == "Longma [Lv. 2000] [Boss]" or MsBoss == "Don Swan [Lv. 1000] [Boss]" or MsBoss == "Cursed Captain [Lv. 1325] [Raid Boss]" or MsBoss == "Order [Lv. 1250] [Raid Boss]" or MsBoss == "rip_indra True Form [Lv. 5000] [Raid Boss]" then
                if game:GetService("ReplicatedStorage"):FindFirstChild(MsBoss) then
                    TP(CFrameBoss)
                end

                    CheckBoss()
                    if game:GetService("Workspace").Enemies:FindFirstChild(MsBoss) then
						for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
							if v.Name == MsBoss and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then

                                TP(v.HumanoidRootPart.CFrame * Farm_Mode)
                                EquipWeapon(MainWeaponName)
                                BusoHaki()
                                Click()
                            end
                       end
					end
                end
            end)
        end
    end
end)


task.spawn(function()
    while task.wait() do
     if _G.Farm_Boss then
        pcall(function()

                CheckBoss()
                if _G.GetQuest then
                if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
                if game:GetService("ReplicatedStorage"):FindFirstChild(MsBoss) or game:GetService("Workspace").Enemies:FindFirstChild(MsBoss) then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuestBoss, LevelQuestBoss)
                end
            end
        end

                CheckBoss()
                if game:GetService("ReplicatedStorage"):FindFirstChild(MsBoss) then
                    TP(CFrameBoss)
                end

                CheckBoss()
                if game:GetService("Workspace").Enemies:FindFirstChild(MsBoss) then
                for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                if v.Name == MsBoss and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        TP(v.HumanoidRootPart.CFrame * Farm_Mode)
                        EquipWeapon(MainWeaponName)
                        BusoHaki()
                        Click()
                                end
                            end
                        end
                    end)
            end
        end
    end)
			
BossFarm:addToggle("Auto Farm All Boss", nil, function(value)
    _G.Farm_BossAll = value
    if value == false then
        TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
        UnClip()
    end
end)

task.spawn(function()
 while task.wait() do
  if _G.Farm_BossAll == true then
pcall(function()  
    for i2, v2 in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
        if string.find(v2.Name, "Boss") then
            if v2.Humanoid.Health > 0 then

            BossTarget = v.Name

            if v.Name == BossTarget and v.Humanoid.Health > 0 then

                TP(v2.HumanoidRootPart.CFrame * Farm_Mode)
                EquipWeapon(MainWeaponName)
                BusoHaki()
                Click() 
            end
            end

    else
    for i, v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
        if string.find(v.Name, "Boss") then

        if 
        v.Name ~= "Soul Reaper [Lv. 2100] [Raid Boss]" 
        and v.Name ~= "Longma [Lv. 2000] [Boss]" 
        and v.Name ~= "Don Swan [Lv. 1000] [Boss]" 
        and v.Name ~= "Cursed Captain [Lv. 1325] [Raid Boss]" 
        and v.Name ~= "Order [Lv. 1250] [Raid Boss]" 
        and v.Name ~= "rip_indra True Form [Lv. 5000] [Raid Boss]"
        then


            if v.Humanoid.Health > 0 then

            BossTarget = v.Name

            if v.Name == BossTarget and v.Humanoid.Health > 0 then

                TP(v.HumanoidRootPart.CFrame * Farm_Mode)
                EquipWeapon(MainWeaponName)
                BusoHaki()
                Click()
            end
            end
            end
        end
            end
        end
    end
    end)
end
end
end)

