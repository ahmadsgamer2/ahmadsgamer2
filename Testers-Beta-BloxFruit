--Raid
Raid:Seperator("Raid")

local Chips = {"Flame","Ice","Quake","Light","Dark","Spider","Rumble","Magma","Buddha","Sand","Phoenix","Dough"}

Raid:Dropdown("Select Chip List",Chips, function(value)
    SelectChip = value
end)

