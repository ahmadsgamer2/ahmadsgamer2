task.spawn(function()
while task.wait() do
if _G.Automatics_SoulGuitar then
pcall(function()

    for i, v in pairs(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("getInventory")) do
    if v.Name ~= "Soul Guitar" then

    if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Bones", "Check") >= 500 then    
    if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Ectoplasm","Check") >= 250 then
    if game:GetService("Players").LocalPlayer.Data.Fragments.Value >= 5000 then
    if v.Name == "Dark Fragment" then
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("soulGuitarBuy")
    end
end
end
end
end
end
    
    if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Bones", "Check") < 500 then

                CFrameQ = CFrame.new(-9516.9931640625, 178.00651550293, 6078.4653320313)

                if (CFrameQ.Position - plr.Position).Magnitude > 350 then
                if FarmSpeed == "Auto Farm Speed : 100 %\n[Dead]" then
                    
            if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value ~= "Ship" then

                    CFrameQ = CFrame.new(-9516.9931640625, 178.00651550293, 6078.4653320313)
                    Enemies = game:GetService("Workspace").Enemies

                    if not Enemies:FindFirstChild("Reborn Skeleton [Lv. 1975]") and not Enemies:FindFirstChild("Living Zombie [Lv. 2000]")
                    and not Enemies:FindFirstChild("Demonic Soul [Lv. 2025]") and not Enemies:FindFirstChild("Posessed Mummy [Lv. 2050]") then

                        pld:ChangeState(15)
                        plr.CFrame = CFrameQ
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetLastSpawnPoint", "Ship")

                        wait()
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetLastSpawnPoint", "Ship")
                    end
                end
            end
        end

            CFrameQ = CFrame.new(-9516.9931640625, 178.00651550293, 6078.4653320313)
            if FarmSpeed == "Auto Farm Speed : 45 %" then
            if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value ~= "Ship" then

            CFrameQ = CFrame.new(-9516.9931640625, 178.00651550293, 6078.4653320313)

            if not game:GetService("Workspace").Enemies:FindFirstChild(Ms) then
                TP(CFrameQ)
            end
        end
    end

    if Enemies:FindFirstChild("Reborn Skeleton [Lv. 1975]") and Enemies:FindFirstChild("Living Zombie [Lv. 2000]")
    and Enemies:FindFirstChild("Demonic Soul [Lv. 2025]") and Enemies:FindFirstChild("Posessed Mummy [Lv. 2050]") then

     Enemies = game:GetService("Workspace").Enemies
    for i, v in pars(Enemies:GetChildren()) do
    if v.Name == "Reborn Skeleton [Lv. 1975]" or v.Name == "Living Zombie [Lv. 2000]" or v.Name == "Demonic Soul [Lv. 2025]" or v.Name == "Posessed Mummy [Lv. 2050]" and v:FindFirstChild("Humanoid") then
    if v.Humanoid.Health > 0 then
    
        if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value == "HauntedCastle" then
            plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
        else
            TP(v.HumanoidRootPart.CFrame * Farm_Mode)
        end

        if _G.BringMonster then

            CheckLevel()
            MagnetSelect(v.Name)
        end

        EquipWeapon(MainWeaponName)
        BusoHaki()
        Click()
    end
    end
    end
    end
    end
end)
end 
end
end)

