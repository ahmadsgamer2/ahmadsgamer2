        Rejoin()
    end)
    local plypage = UI:addPage("Players\nBounty", 7252023075)
    local plysection = plypage:addSection("Players")
    local playerlist = {}
    
    for i,v in pairs(game.Players:GetPlayers())do
       if v ~= game.Players.LocalPlayer then
           table.insert(playerlist,v.Name)
       end
    end
    
    local playerdrop = plysection:addDropdown("Select Player : None", playerlist, function(abc)
     Player = abc
    end)
    
    game.Players.PlayerAdded:Connect(function(player)
       local name = player.Name
       table.insert(playerlist,name)
       playerdrop:Refresh(playerlist)
    end)
    
    game.Players.PlayerRemoving:Connect(function(player)
       local name = player.Name
       for i,v in pairs(playerlist)do
           if v == name then  
               table.remove(playerlist,i)
           end
       end
       playerdrop:Refresh(playerlist)
    end)
    plysection:addToggle("Kill Player", nil, function(value)
    _G.KillPlayer = value
    if value == false then
        UnClip()
        TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
    end
    end)
    plysection:addToggle("Fast Attack", nil,function(a)
        _G.FastAttack = a
    end)
    plysection:addToggle("Spectate Player", nil,function(Spectate)
        Sp = Spectate
        local plr1 = game.Players.LocalPlayer.Character.Humanoid
        local plr2 = game.Players:FindFirstChild(Player)
        repeat wait(.1)
            game.Workspace.Camera.CameraSubject = plr2.Character.Humanoid
        until Sp == false 
        game.Workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
    end)
    plysection:addToggle("Aimbot Gun", nil,function(a)
        _G.AimbotGun = a
    end)
    
        task.spawn(function()
            while task.wait() do
            if _G.AimbotGun then
                pcall(function()
                if game:GetService("Players").LocalPlayer.Character:FindFirstChild(GunName) and game.Players:FindFirstChild(Player) then

                    game:GetService("Players").LocalPlayer.Character[GunName].Cooldown.Value = 0
