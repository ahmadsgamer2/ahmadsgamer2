--ESP

ESP:Seperator("ESP")

ESP:Toggle("ESP Player",false,function(Value)
    ESPPlayer = Value
	UpdatePlayerChams()
end)

ESP:Toggle("ESP Fruit Devil",false,function(Value)
    DevilFruitESP = Value
    while DevilFruitESP do wait()
        UpdateDevilChams() 
    end
end)

ESP:Toggle("ESP Island",false,function(Value)
    IslandESP = Value
    while IslandESP do wait()
        UpdateIslandESP() 
    end
end)

ESP:Toggle("ESP Flower",false,function(Value)
    FlowerESP = Value
	UpdateFlowerChams() 
end)

ESP:Toggle("ESP Real Fruit",false,function(Value)
    RealFruitESP = Value
	UpdateRealFruitChams() 
end)

ESP:Toggle("ESP Mob",false,function(Value)
    MobESP = Value
end)

ESP:Toggle("ESP Sea",false,function(Value)
    SeaESP = Value
end)

ESP:Toggle("ESP NPC",false,function(Value)
    NpcESP = Value
end)

ESP:Toggle("ESP Advanced Fruit Dealer",false,function(Value)
    AfdESP = Value
    UpdateAfdESP()
end)

ESP:Toggle("ESP Master of Enhancement",false,function(Value)
    AuraESP = Value
    UpdateAuraESP()
end)

ESP:Toggle("ESP Legendary Sword Dealer",false,function(Value)
    LADESP = Value
    UpdateLSDESP()
end)

ESP:Toggle("ESP Gear",false,function(Value)
    GearESP = Value
    UpdateGeaESP()
end)

spawn(function()
    while wait(2) do
        if FlowerESP then
            UpdateFlowerChams() 
        end
        if DevilFruitESP then
            UpdateDevilChams() 
        end
        if ChestESP then
            UpdateChestChams() 
        end
        if ESPPlayer then
            UpdatePlayerChams()
        end
        if RealFruitESP then
            UpdateRealFruitChams()
        end
        if AfdESP then
            UpdateAfdESP()
        end
        if AuraESP then
            UpdateAuraESP()
        end
        if LADESP then
            UpdateLSDESP()
        end
        if GearESP then
            UpdateGeaESP()
        end
    end
end)

