if _G.Chest_Farm then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ahmadsgamer2/ahmadsgamer2/main/ChestFarm_BF.lua"))()
elseif _G.FruitFinder then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ahmadsgamer2/ahmadsgamer2/main/FruitFinder.lua"))()
else

local Update = loadstring(game:HttpGet("https://raw.githubusercontent.com/bot21333/C-/main/ZekromHubX_V2_UIC%2B%2B.lua"))()
local Library = Update:Window()
local Information_Tab = Library:CreateTab({
    Name = LabelInformation_Translate
})
local Information_Section = Information_Tab:CreateSection({
    Name = "Main",
    Side = "Left"
})

local Translate = {
    "English",
    "Türkçe",
    "Azəri Türkcəsi",
    "Español",
    "Latviski",
    "Nederlands",
    "Filipino",
    "Deustch",
    "Română",
    "Русский",
    "Français",
    "عربي",
}

Information_Section:AddDropdown({
    Name = "Select Translate",
    Flag = "Select_Translate",
    Value = "English",
    List = Translate,
    Callback = function(value)
        Select_Translate = value
    end
})

    if Select_Translate == "English" then -----[ English ]
            LabelInformation_Translate = "Information"
            LabelFps_Translate = "Fps : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        elseif Select_Translate == "Türkçe" then -----[ Türkçe ]
            LabelInformation_Translate = "bilgi"
            LabelFps_Translate = "fps'de : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        elseif Select_Translate == "Azəri Türkcəsi" then -----[ Azəri Türkcəsi ]
            LabelInformation_Translate = "Məlumat"
            LabelFps_Translate = "Fps : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        elseif Select_Translate == "Español" then -----[ Español ]
            LabelInformation_Translate = "información"
            LabelFps_Translate = "Fps : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        elseif Select_Translate == "Latviski" then -----[ Latviski ]
            LabelInformation_Translate = "Informācija"
            LabelFps_Translate = "Fps : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        elseif Select_Translate == "Nederlands" then -----[ Nederlands ]
            LabelInformation_Translate = "Informatie"
            LabelFps_Translate = "Fps : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        elseif Select_Translate == "Filipino" then -----[ Filipino ]
            LabelInformation_Translate = "Məlumat"
            LabelFps_Translate = "Fps : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        elseif Select_Translate == "Deustch" then -----[ Deustch ]
            LabelInformation_Translate = "Information"
            LabelFps_Translate = "Fps : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        elseif Select_Translate == "Română" then -----[ Română ]
            LabelInformation_Translate = "informație"
            LabelFps_Translate = "Fps : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        elseif Select_Translate == "Русский" then -----[ Русский ]
            LabelInformation_Translate = "Информация"
            LabelFps_Translate = "Fps : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        elseif Select_Translate == "Français" then -----[ Français ]
            LabelInformation_Translate = "información"
            LabelFps_Translate = "Fps : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        elseif Select_Translate == "عربي" then -----[ عربي ]
            LabelInformation_Translate = "معلومة"
            LabelFps_Translate = "Fps : "
            LabelPing_Translate = "Ping : "
            LabelDiscord_Translato = "Join Discord - Zekrom Hub X"
        end

local Client  = Information_Section:AddLabel({Name = "..."})

function UpdateClient()
    local Fps = workspace:GetRealPhysicsFPS()
    Client:Set(LabelFps_Translate..Fps)
end
    
spawn(function()
    while true do wait(.1)
        UpdateClient()
    end
end)

local Client1  = Information_Section:AddLabel({Name = "..."})

function UpdateClient1()
    local Ping = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
    Client1:Set(LabelPing_Translate..Ping)
end
    
spawn(function()
    while true do wait(.1)
        UpdateClient1()
    end
end)

Information_Section:AddButton({
	Name = LabelDiscord_Translato,
	Callback = function()
		setclipboard("https://discord.gg/NdY3TXK6Ex")
	end
})

end
