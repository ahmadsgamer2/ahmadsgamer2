local LocalPlSec = dfpage:addSection("Local Player")

LocalPlSec:addToggle("No Sky Jump Limit", nil, function(value)
    _G.NgCd = value
        NoGeppoLimit()
   end)

LocalPlSec:addToggle("No Dodge Cooldown", nil, function(value)
 _G.NdCd = value
    NoDodgeCd()
end)


LocalPlSec:addToggle("No Observation Radius Limit", nil, function(value)
RadiusLimit = value
while RadiusLimit do wait()
    game:GetService("Players").LocalPlayer.VisionRadius.Value = math.huge
end
end)

function NoGeppoLimit()
	if _G.NgCd then
		for i,v in next, getgc() do
			if game.Players.LocalPlayer.Character.Geppo then
				if typeof(v) == "function" and getfenv(v).script == game.Players.LocalPlayer.Character.Geppo then
					for i2,v2 in next, getupvalues(v) do
						if tostring(v2) == "0" then
							repeat wait(.1)
								setupvalue(v,i2,0)
							until not _G.NgCd
						end
					end
				end
			end
		end
	end
end

function NoDodgeCd()
	if _G.NdCd then
		for i,v in next, getgc() do
			if game.Players.LocalPlayer.Character.Dodge then
				if typeof(v) == "function" and getfenv(v).script == game.Players.LocalPlayer.Character.Dodge then
					for i2,v2 in next, getupvalues(v) do
						if tostring(v2) == "0.4" then
							repeat wait(.1)
								setupvalue(v,i2,0)
							until not _G.NdCd
						end
					end
				end
			end
		end
	end
end

local Basic = dfpage:addSection("Basic")

WalkSpeed = 1
Basic:addSlider("Walk Speed", 1,-32,1250, function(value)
    WalkSpeed = value
end)

JumpPower = 50
Basic:addSlider("Jump Power", 50,-50,1250, function(value)
    JumpPower = value
end)

Basic:addButton("Check Current Walk Speed", function(value)
    UI:Notify("Impact Hub", "Your Current Walk Speed Is : "..game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed)
end)

Basic:addButton("Check Current Jump Power", function(value)
    UI:Notify("Impact Hub", "Your Current Jump Power Is : "..game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower)
end)

Basic:addToggle("Set Walk Speed", nil, function(value)
    _G.SetWS = value
end)
Basic:addToggle("Set Jump Power", nil, function(value)
    _G.SetJP = value
end)

Basic:addButton("Walk Speed Bypass", function()
    game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(15)
end)

spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
    if _G.SetWS then
     pcall(function()
        game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed += WalkSpeed
     end)
     end
    end)
    end)

    spawn(function()
        game:GetService("RunService").RenderStepped:Connect(function()
        if _G.SetJP then
         pcall(function()
            game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower += JumpPower
         end)
         end
        end)
        end)


    task.spawn(function()
        while task.wait() do
            if _G.SetWS then
                pcal(function()
                game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed += WalkSpeed
            end)
        end
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.SetJP then
            pcal(function()
            game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower += JumpPower
        end)
    end
end
end)



local Character = dfpage:addSection("Character")

Character:addToggle("Auto Click", nil, function(value)
_G.AutoC = value
while _G.AutoC do wait()
    game:GetService'VirtualUser':CaptureController()
    game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
end
end)

FlySpeed = 250
Character:addSlider("Fly Speed", 250,200,1250, function(value)
    FlySpeed = value
end)

FlyBind = "g"
Character:addTextbox("[Only Small Letter] Fly Key", "g", function(value)
    FlyBind = value
end)

        Character:addToggle("Fly", nil, function(value)
        FL = value
        local Max = 0
        local Players = game.Players
        local LP = Players.LocalPlayer
        local Mouse = LP:GetMouse()
        Mouse.KeyDown:connect(
            function(k)
                if k:lower() == FlyBind then
                    Max = Max + 1
                    getgenv().Fly = false
                    if FL then
                        local T = LP.Character.UpperTorso
                        local S = {
                            F = 0,
                            B = 0,
                            L = 0,
                            R = 0
                        }
                        local S2 = {
                            F = 0,
                            B = 0,
                            L = 0,
                            R = 0
                        }
                        local SPEED = 45
                        local function FLY()
                            getgenv().Fly = true
                            local BodyGyro = Instance.new("BodyGyro", T)
                            local BodyVelocity = Instance.new("BodyVelocity", T)
                            BodyGyro.P = 9e4
                            BodyGyro.maxTorque = Vector3.new(9e9, 9e9, 9e9)
                            BodyGyro.cframe = T.CFrame
                            BodyVelocity.velocity = Vector3.new(0, 0.1, 0)
                            BodyVelocity.maxForce = Vector3.new(9e9, 9e9, 9e9)
                            spawn(
                                function()
                                    repeat
                                        wait()
                                        LP.Character.Humanoid.PlatformStand = false
                                        if S.L + S.R ~= 0 or S.F + S.B ~= 0 then
                                            SPEED = FlySpeed
                                        elseif not (S.L + S.R ~= 0 or S.F + S.B ~= 0) and SPEED ~= 0 then
                                            SPEED = 0
                                        end
                                        if (S.L + S.R) ~= 0 or (S.F + S.B) ~= 0 then
                                            BodyVelocity.velocity =
                                                ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (S.F + S.B)) +
                                                ((game.Workspace.CurrentCamera.CoordinateFrame *
                                                    CFrame.new(S.L + S.R, (S.F + S.B) * 0.2, 0).p) -
                                                    game.Workspace.CurrentCamera.CoordinateFrame.p)) *
                                                SPEED
                                            S2 = {
                                                F = S.F,
                                                B = S.B,
                                                L = S.L,
                                                R = S.R
                                            }
                                        elseif (S.L + S.R) == 0 and (S.F + S.B) == 0 and SPEED ~= 0 then
                                            BodyVelocity.velocity =
                                                ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (S2.F + S2.B)) +
                                                ((game.Workspace.CurrentCamera.CoordinateFrame *
                                                    CFrame.new(S2.L + S2.R, (S2.F + S2.B) * 0.2, 0).p) -
                                                    game.Workspace.CurrentCamera.CoordinateFrame.p)) *
                                                SPEED
                                        else
                                            BodyVelocity.velocity = Vector3.new(0, 0.1, 0)
                                        end
                                        BodyGyro.cframe = game.Workspace.CurrentCamera.CoordinateFrame
                                    until not getgenv().Fly
                                    S = {
                                        F = 0,
                                        B = 0,
                                        L = 0,
                                        R = 0
                                    }
                                    S2 = {
                                        F = 0,
                                        B = 0,
                                        L = 0,
                                        R = 0
                                    }
                                    SPEED = 0
                                    BodyGyro:destroy()
                                    BodyVelocity:destroy()
                                    LP.Character.Humanoid.PlatformStand = false
                                end
                            )
                        end
                        Mouse.KeyDown:connect(
                            function(k)
                                if k:lower() == "w" then
                                    S.F = 1
                                elseif k:lower() == "s" then
                                    S.B = -1
                                elseif k:lower() == "a" then
                                    S.L = -1
                                elseif k:lower() == "d" then
                                    S.R = 1
                                end
                            end
                        )
                        Mouse.KeyUp:connect(
                            function(k)
                                if k:lower() == "w" then
                                    S.F = 0
                                elseif k:lower() == "s" then
                                    S.B = 0
                                elseif k:lower() == "a" then
                                    S.L = 0
                                elseif k:lower() == "d" then
                                    S.R = 0
                                end
                            end
                        )
                        FLY()
                        if Max == 2 then
                            getgenv().Fly = false
                            Max = 0
                        end
                    end
                end
            end
        )
 
        end)
        Character:addToggle("Noclip", nil, function(value)
            _G.Noclip = value
            end)
        Character:addToggle("Float", nil, function(value)
            _G.Float = value
            if value == false then
                UnClip()
              end
            end)

        Character:addButton("Full Bright", function()
            loadstring(game:HttpGet("https://pastebin.com/raw/06iG6YkU", true))()
        end)

_G.AFK = true
Character:addToggle("Anti AFK", true, function(value)
        _G.AFK = value
        if _G.AFK then
        while _G.AFK do wait()
        local vu = game:GetService("VirtualUser")
        game:GetService("Players").LocalPlayer.Idled:connect(function()
        vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        wait(1)
        vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)

        end)
    end
end
end)


local Auto_Mat = UI:addPage("Automatics\nMaterial", 6023426909)

local Automatics = Auto_Mat:addSection("More Automatics")

Automatics:addToggle("Auto Speed Farm Money", nil, function(value)
    _G.Farm_SpeedMoney = value
    if value == false then
        TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
        UnClip()
    end
end)

task.spawn(function()
    while task.wait() do
        if _G.Farm_SpeedMoney or _G.Farm_SpeedMastery then
        pcall(function()
            if Three_World then
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            local pld = game.Players.LocalPlayer.Character.Humanoid

            if (CFrame.new(-2069, 68, -12946).Position - plr.Position).Magnitude > 350 then
            if FarmSpeed == "Auto Farm Speed : 100 %\n[Dead]" then
    
            if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value ~= "Loaf" then
                    pld:ChangeState(15)
                    plr.CFrame = CFrame.new(-2434, 132, -12110)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetLastSpawnPoint", "Loaf")
    
                    wait()
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetLastSpawnPoint", "Loaf")
                    end
                end
            end

        local Ene = game:GetService("Workspace").Enemies
        
        if Ene:FindFirstChild("Cookie Crafter [Lv. 2200]") or Ene:FindFirstChild("Cake Guard [Lv. 2225]") or Ene:FindFirstChild("Baking Staff [Lv. 2250]") or Ene:FindFirstChild("Head Baker [Lv. 2275]") then
        for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
        if v.Name == "Head Baker [Lv. 2275]" or v.Name == "Baking Staff [Lv. 2250]" or v.Name == "Cake Guard [Lv. 2225]" or v.Name == "Cookie Crafter [Lv. 2200]" and v:FindFirstChild("Humanoid") then
        if v.Humanoid.Health > 0 then
        TP(v.HumanoidRootPart.CFrame * Farm_Mode)

        if _G.BringMonster then
        MagnetSelect(v.Name)
        end

        BusoHaki()
        EquipWeapon(MainWeaponName)
        Click()
                    end
                end
            end
            end
            end

        if New_World then

            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            local pld = game.Players.LocalPlayer.Character.Humanoid
            local Ene = game:GetService("Workspace").Enemies

            if not Ene:FindFirstChild("Ship Engineer [Lv. 1275]") and not Ene:FindFirstChild("Ship Deckhand [Lv. 1250]") and not Ene:FindFirstChild("Ship Officer [Lv. 1325]") and not Ene:FindFirstChild("Ship Steward [Lv. 1300]") then
            if FarmSpeed == "Auto Farm Speed : 100 %\n[Dead]" then
    
            CheckLevel()        
            if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value ~= "Ship" then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", Vector3.new(923.2125244140625, 126.97600555419922, 32852.83203125))
                    end
                end
            end

        local Ene = game:GetService("Workspace").Enemies
        
        if Ene:FindFirstChild("Ship Engineer [Lv. 1275]") or Ene:FindFirstChild("Ship Deckhand [Lv. 1250]") or Ene:FindFirstChild("Ship Officer [Lv. 1325]") or Ene:FindFirstChild("Ship Steward [Lv. 1300]") then

        for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
        if v.Name == "Ship Deckhand [Lv. 1250]" or v.Name == "Ship Engineer [Lv. 1275]" or v.Name == "Ship Officer [Lv. 1325]" or v.Name == "Ship Steward [Lv. 1300]" and v:FindFirstChild("Humanoid") then
        if v.Humanoid.Health > 0 then

        wait(0.3)
        TP(v.HumanoidRootPart.CFrame * Farm_Mode)

        if _G.BringMonster then
        MagnetSelect(v.Name)
        end

        BusoHaki()
        EquipWeapon(MainWeaponName)
        Click()
                            end
                        end
                    end
                end
            end
        end)
    end
end
end)

