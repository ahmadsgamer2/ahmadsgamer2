            end 
        end
    end)
end)

DevilFruit:Toggle("Auto Collect Fruit",false,function(value)
    _G.Tweenfruit = value
end)

spawn(function()
    while wait(.1) do
        if _G.Tweenfruit then
            for i,v in pairs(game.Workspace:GetChildren()) do
                if string.find(v.Name, "Fruit") then
                    Tween(v.Handle.CFrame)
                end
            end
        end
    end
end)
