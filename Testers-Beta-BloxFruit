                    local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
                    local pld = game.Players.LocalPlayer.Character.Humanoid

                if _G.GetQuest then
                    if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuest, QuestLv)
                    end
                end
                        CheckLevel()
                        if (CFrameMon.Position - plr.Position).Magnitude > 350 then
                            if FarmSpeed == "Auto Farm Speed : 100 %\n[Dead]" then
                            
                                CheckLevel()
                                if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value ~= SpawnValue then
                                        if not game:GetService("Workspace").Enemies:FindFirstChild(SelectMonster) then
                                            CheckLevel()
                                            pld:ChangeState(15)
                                            plr.CFrame = CFrameQ
                                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetLastSpawnPoint", SpawnValue)
                                            
                                            wait()
                                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetLastSpawnPoint", SpawnValue)
                                        end
                                    end
                                end
                            end
                
                CheckLevel()
                if not game:GetService("Workspace").Enemies:FindFirstChild(SelectMonster) then
                    plr.CFrame = CFrameMon
                end
        
                    if game:GetService("Workspace").Enemies:FindFirstChild(SelectMonster) then

                        for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                            if v.Name == SelectMonster and v:FindFirstChild("Humanoid") then
                                    if v.Humanoid.Health > 0 then
                                        TP(v.HumanoidRootPart.CFrame * Farm_Mode)
        
