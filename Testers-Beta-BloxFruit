if getgenv().JoinZekrom == true then
    local JobID = game.JobId
    script = string.gsub(script, "ZekromHubX_", ""..JobID)

    game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId,)
end

local JobID = game.JobId
