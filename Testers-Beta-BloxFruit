--Setting

Settings_1:Toggle("Bypass Anti-Cheat: Anti Flag Reset",true,function(value)
    _G.AntiFlagReset = value
end)

spawn(function()
    while wait(2000) do
        if _G.AntiFlagReset then
            pcall(function()
            game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
        end)
    end
   end
end)

spawn(function()
    while wait() do
        if _G.AntiFlagReset then
            pcall(function()
                if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
                    local HealthPercent = game.Players.LocalPlayer.Character.Humanoid.Health / game.Players.LocalPlayer.Character.Humanoid.MaxHealth * 100
                    if HealthPercent < 20 then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 100, 0)
                    end
                end
                task.wait()
            end)
            end
        end
    end)

    local Players = game:GetService("Players")

    local function antiResetData(player)
        local function onCharacterAdded(character)
            local humanoid = character:WaitForChild("Humanoid")
            humanoid.Died:Connect(function()
            end)
        end
    
        player.CharacterAdded:Connect(onCharacterAdded)
        if player.Character then
            onCharacterAdded(player.Character)
        end
    end
    
    Players.PlayerAdded:Connect(antiResetData)
    for _, player in ipairs(Players:GetPlayers()) do
        antiResetData(player)
    end

Settings_1:Toggle("Bring Mob",true,function(value)
    _G.BringMob = value
end)
task.spawn(function()
    while task.wait() do
    if _G.BringMob and bringmob then
    pcall(function()
      for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
      if not string.find(v.Name,"Boss") and v.Name == MonFarm and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 350 then
      if InMyNetWork(v.HumanoidRootPart) then
        if InMyNetWork(v.HumanoidRootPart) then
      v.HumanoidRootPart.CFrame = FarmPos
      v.HumanoidRootPart.CanCollide = false
      v.HumanoidRootPart.Size = Vector3.new(1,1,1)
      if v.Humanoid:FindFirstChild("Animator") then
        v.Humanoid.Animator:Destroy()
    end
      end
    end
      end
      end
      end)
    end
end
end)

spawn(function()
    while wait() do
        pcall(function()
            for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                if _G.BringMob and bringmob then
                    if v.Name == MonFarm and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        if v.Name == "Factory Staff" then
                            if (v.HumanoidRootPart.Position - FarmPos.Position).Magnitude <= 500 then
                                v.Head.CanCollide = false
                                v.HumanoidRootPart.CanCollide = false
                                v.HumanoidRootPart.Size = Vector3.new(1, 1, 1)
                                v.HumanoidRootPart.CFrame = FarmPos
                                if v.Humanoid:FindFirstChild("Animator") then
                                    v.Humanoid.Animator:Destroy()
                                end
                                sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                            end
                        elseif v.Name == MonFarm then
                            if (v.HumanoidRootPart.Position - FarmPos.Position).Magnitude <= 450 then
                                v.Head.CanCollide = false
                                v.HumanoidRootPart.CanCollide = false
                                v.HumanoidRootPart.Size = Vector3.new(1, 1, 1)
                                v.HumanoidRootPart.CFrame = FarmPos
                                if v.Humanoid:FindFirstChild("Animator") then
                                    v.Humanoid.Animator:Destroy()
                                end
                                sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
                            end
                        end
                    end
                            end
                        end
                    end)
            end
        end)



        Settings_1:Toggle("Bypass Teleport",false,function(value)
            BypassTP = value
        end)

        Settings_1:Toggle("Remove Damage Text",false,function(value)
            Removetext = value
        end)

        spawn(function()
            while wait() do
                if Removetext then
                    game:GetService("ReplicatedStorage").Assets.GUI.DamageCounter.Enabled = false
                else
                    game:GetService("ReplicatedStorage").Assets.GUI.DamageCounter.Enabled = true
                end
            end
        end)

        Settings_1:Toggle("Remove Notify",false,function(value)
            RemoveNotify = value
        end)

        spawn(function()
            while wait() do
                if RemoveNotify then
                    game.Players.LocalPlayer.PlayerGui.Notifications.Enabled = false
                else
                    game.Players.LocalPlayer.PlayerGui.Notifications.Enabled = true
                end
            end
        end)

        Settings_1:Toggle("White Screen",false,function(value)
            _G.WhiteScreen = value
            if _G.WhiteScreen == true then
             game:GetService("RunService"):Set3dRenderingEnabled(false)
         elseif _G.WhiteScreen == false then
             game:GetService("RunService"):Set3dRenderingEnabled(true)
            end
        end)

        Settings_1:Seperator("Skill Mastery")

        Settings_1:Toggle("Skill Z",true,function(value)
            SkillZ = value
        end)

        Settings_1:Toggle("Skill X",true,function(value)
            SkillX = value
        end)

        Settings_1:Toggle("Skill C",true,function(value)
            SkillC = value
        end)

        Settings_1:Toggle("Skill V",true,function(value)
            SkillV = value
        end)

        Settings_1:Toggle("Skill F",true,function(value)
            SkillF = value
        end)

	end

