task.spawn(function()
    while task.wait() do
     if _G.Fk_WaterKey then
      pcall(function()
    if not game:GetService("ReplicatedStorage"):FindFirstChild("Tide Keeper [Lv. 1475] [Boss]") then
    if _G.Hop then
        Hop()
    end
    elseif game:GetService("ReplicatedStorage"):FindFirstChild("Tide Keeper [Lv. 1475] [Boss]") then
            if not game:GetService("Workspace").Enemies:FindFirstChild("Tide Keeper [Lv. 1475] [Boss]") then
                TP(game:GetService("ReplicatedStorage")["Tide Keeper [Lv. 1475] [Boss]"].HumanoidRootPart.CFrame * CFrame.new(0,35,35))
            end
            if game:GetService("Workspace").Enemies:FindFirstChild("Tide Keeper [Lv. 1475] [Boss]") then
                for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                    if v.Name == "Tide Keeper [Lv. 1475] [Boss]" then    
                    if v.Humanoid.Health > 0 then
                        local plr = game.Players.LocalPlayer.Character.HumanoidRootPart

                        if (v.HumanoidRootPart.Position - plr.Position).Magnitude < 350 then
                            plr.CFrame = v.HumanoidRootPart.CFrame * Farm_Mode
                        else
                            TP(v.HumanoidRootPart.CFrame * Farm_Mode)
                        end

                        EquipWeapon(MainWeaponName)
                        BusoHaki()
                        Click()
                    end
                end
            end
         end
      end
    end)
    end
end
end)
