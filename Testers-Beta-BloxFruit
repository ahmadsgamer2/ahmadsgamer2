local MirageIsland = game.Workspace._WorldOrigin.Locations:FindFirstChild('Mirage Island')

while true do
    for _, server in ipairs(game:GetService("TeleportService"):GetLocalPlayerTeleportOptions(MirageIsland.PlaceId)) do
        if server.CurrentPlayers < server.MaxPlayers then
            game:GetService("TeleportService"):TeleportToPlaceInstance(MirageIsland.PlaceId, server.PlaceId)
            break
        end
    end
    wait(1)
end
