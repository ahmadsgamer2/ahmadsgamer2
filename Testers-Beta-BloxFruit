        game:GetService("RunService").Stepped:Connect(function()
            if _G.ChestFarm1 then
                for i, v in pairs(game.Workspace:GetChildren()) do
                    if v.Name == "Chest1" and v:FindFirstChild("Mesh") then
                        if (v.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < DisChest then
                            TP(v.CFrame)
                           end
                        end
                    end
                end
            end)
    end)
end)

