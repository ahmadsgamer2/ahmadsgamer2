    _G.TeleportIsland = value
    if _G.TeleportIsland then
    if World1  then
        if _G.SelectLocalTeleport == "Jones Salad" then
            toTarget(CFrame.new(1042.1501464844, 16.299360275269, 1444.3442382813))
        end
