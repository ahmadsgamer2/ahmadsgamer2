end)

task.spawn(function()
    while task.wait() do 
        if _G.Swords_Buddy then
        pcall(function()
        local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
        
        
        if not game:GetService("ReplicatedStorage"):FindFirstChild("Cake Queen [Lv. 2175] [Boss]") and not game:GetService("Workspace").Enemies:FindFirstChild("Cake Queen [Lv. 2175] [Boss]") then
        if _G.Hop_Sword then
            Hop()
        end
        end

        if game:GetService("ReplicatedStorage"):FindFirstChild("Cake Queen [Lv. 2175] [Boss]") then
            FastTP("IceCream", "Ice Cream Chef [Lv. 2125]", CFrame.new(-760, 66, -10865))
        end

        if game:GetService("Workspace").Enemies:FindFirstChild("Cake Queen [Lv. 2175] [Boss]") then

        if _G.GetQuest then
        if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", "IceCreamIslandQuest", 3)
        end

        if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
            RightQuest("Cake Queen")
        end

        for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
        if v.Name == "Cake Queen [Lv. 2175] [Boss]" and v:FindFirstChild("Humanoid") then
        if v.Humanoid.Health > 0 then

            TP(v.HumanoidRootPart.CFrame * Farm_Mode) 

            BusoHaki()
            EquipWeapon(MainWeaponName)
            Click()
                    end
                end
            end
            end
        end
        end)
    end
end
end)

