task.spawn(function()
    while task.wait() do
    if _G.Material_Ectoplasm then
    pcall(function()
        local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
        local pld = game.Players.LocalPlayer.Character.Humanoid

        if _G.GetQuest then
            if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", "ShipQuest2", 2)
            end
        end

        local Enemies = game:GetService("Workspace").Enemies
    
        if not Enemies:FindFirstChild("Ship Deckhand [Lv. 1250]") then
        if not Enemies:FindFirstChild("Ship Officer [Lv. 1325]") then
        if not Enemies:FindFirstChild("Ship Steward [Lv. 1300]") then
        if not Enemies:FindFirstChild("Ship Engineer [Lv. 1275]") then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", Vector3.new(923.2125244140625, 126.97600555419922, 32852.83203125))
        end
    end
    end
    end
    
    local Enemies = game:GetService("Workspace").Enemies
    
    for i, v in pairs(Enemies:GetChildren()) do
    
    if string.find(v.Name, "Ship") and v:FindFirstChild("Humanoid") then
    if v.Humanoid.Health > 0 then
    wait(1.8)
    TP(v.HumanoidRootPart.CFrame * Farm_Mode)
    end
    
    if _G.BringMonster then
        MagnetSelect(v.Name)
    end
    
    BusoHaki()
    EquipWeapon(MainWeaponName)
    Click()
    
    end
    end
    end)
    end
    end
    end)

    task.spawn(function()
        while task.wait() do
        if _G.Material_Bone then
        pcall(function()
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            local pld = game.Players.LocalPlayer.Character.Humanoid

            if _G.GetQuest then
                if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", "HauntedQuest"..math.random(1,2), math.random(1,2))
                end
            end

            local Enemies = game:GetService("Workspace").Enemies
        
            if not Enemies:FindFirstChild("Reborn Skeleton [Lv. 1975]") then
            if not Enemies:FindFirstChild("Living Zombie [Lv. 2000]") then
            if not Enemies:FindFirstChild("Demonic Soul [Lv. 2025]") then
            if not Enemies:FindFirstChild("Posessed Mummy [Lv. 2050]") then

            if FarmSpeed == "Auto Farm Speed : 45 %" then
                TP(CFrame.new(-9515.3720703125, 164.00624084473, 5786.0610351562))
            end

            if FarmSpeed == "Auto Farm Speed : 100 %\n[Dead]" then
            if game:GetService("Players").LocalPlayer.Data.LastSpawnPoint.Value ~= "HauntedCastle" then

                    pld:ChangeState(15)
                    plr.CFrame = CFrame.new(-9515.3720703125, 164.00624084473, 5786.0610351562)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetLastSpawnPoint", "HauntedCastle")
                    wait()
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetLastSpawnPoint", "HauntedCastle")
                end
            end
        end
    end
end
end
        
        local Enemies = game:GetService("Workspace").Enemies
        
        for i, v in pairs(Enemies:GetChildren()) do
        
        if v.Name == "Reborn Skeleton [Lv. 1975]" or v.Name == "Living Zombie [Lv. 2000]" or v.Name == "Demonic Soul [Lv. 2025]" or v.Name == "Posessed Mummy [Lv. 2050]" and v:FindFirstChild("Humanoid") then
        if v.Humanoid.Health > 0 then

        TP(v.HumanoidRootPart.CFrame * Farm_Mode)
        end

        
        if _G.BringMonster then
            MagnetSelect(v.Name)
        end
        
        BusoHaki()
        EquipWeapon(MainWeaponName)
        Click()
        
        end
        end
        end)
        end
        end
        end)

